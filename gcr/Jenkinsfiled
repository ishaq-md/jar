pipeline {
    agent any

    environment {
    SVC_ACCOUNT_KEY = credentials('terraform-auth')
  }
     
    stages {
        stage ('checkout'){
            steps {
                git branch: 'feature/mi01', url: 'https://github.com/ishaqmdgcp/jar.git'
            }
        }
      
      	stage('Set creds') {
            steps {
              
                sh 'echo $SVC_ACCOUNT_KEY | base64 -d > ./gcr/jenkins.json'
		            sh 'pwd'
                       
               
            }
        }
	
	
        
       
	 
	stage('Delete GKE cluster') {
	 steps {
    
        sh 'gcloud container clusters delete sample-cluster --zone us-central1-a --project mi-env-dev'
    }
    }
    
        
    
    
       }
}
